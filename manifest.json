{
  "manifest_version": 3,
  "name": "Redirect Shorts",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "main.html"
  },
  "icons": {
    "16": "./icons/icon16.png",
    "32": "./icons/icon32.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}